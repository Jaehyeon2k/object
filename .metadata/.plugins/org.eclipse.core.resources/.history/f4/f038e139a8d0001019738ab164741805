package p251204동기화;

class Counter {
	int value;
	
	void inc() {
		value++;
	}
	
	void dec() {
		value--;
	}
	
	void print() {
		System.out.println(value);
	}
}

// Thread 클래스
class MyThread extends Thread{
	Counter c;
	
	void MyThread(Counter c) {
		this.c = c;
	}
}
public class Abb {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
